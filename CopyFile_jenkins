pipeline{
  agent any
 parameters{
    string(name: 'source', defaultValue:"MyStr", description:'Source Path')
    string(name: 'destination', defaultValue:"MyStr", description:'Destination Path')

}
stage('Checkout') {
    steps {
        checkout scm
    }
}

    stages{
        stage('CopyFile'){
           steps{

             powershell './CopyFile.ps1'

}
}
}
}
